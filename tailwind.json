{
	"static": {
		"style": {
			"position": "static"
		}
	},
	"fixed": {
		"style": {
			"position": "fixed"
		}
	},
	"absolute": {
		"style": {
			"position": "absolute"
		}
	},
	"relative": {
		"style": {
			"position": "relative"
		}
	},
	"mt-2": {
		"style": {
			"marginTop": 8
		}
	},
	"block": {
		"style": {
			"display": "block"
		}
	},
	"inline": {
		"style": {
			"display": "inline"
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"table": {
		"style": {
			"display": "table"
		}
	},
	"contents": {
		"style": {
			"display": "contents"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"flex-[1]": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": 0
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"bg-red-200": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(254 202 202 / var(--tw-bg-opacity))"
		}
	},
	"bg-black": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(0 0 0 / var(--tw-bg-opacity))"
		}
	},
	"text-lg": {
		"style": {
			"fontSize": 18,
			"lineHeight": 28
		}
	},
	"font-bold": {
		"style": {
			"fontWeight": "700"
		}
	},
	"uppercase": {
		"style": {
			"textTransform": "uppercase"
		}
	},
	"capitalize": {
		"style": {
			"textTransform": "capitalize"
		}
	},
	"italic": {
		"style": {
			"fontStyle": "italic"
		}
